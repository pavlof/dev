<!DOCTYPE HTML>
<html lang="en">
<head>
    <title>GroupTree Global</title>
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, width=device-width">
    <link href="/assets/styles/main.css" rel="stylesheet" type="text/css" />
    <script src="/assets/scripts/lib/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="/assets/scripts/lib/jquery.iosslider.min.js" type="text/javascript"></script>
    <script src="/assets/scripts/functions.js" type="text/javascript"></script>
    <script src="/assets/scripts/main.js" type="text/javascript"></script>
</head>
<body>
    <div class="b6">
        <div class="gt-container content">
            <!--content top -->
            <div class="gtbox-12">
                <div class="inner mt0 mb0">
                    <p class="m0 c2">Contact us: <span class="c1">020 7404 3737</span></p>
                </div>
            </div>
            <div class="clearfix">
            </div>
        </div>
    </div>
    <!--content -->
    <div class="gt-container content">
        <!--content top -->
        <div class="gtbox-12">
            <div class="inner">
                <div id="logo" class="left-float">
                    <a href="/" class="logotype">
                        <img src="http://grouptree.com/files/uploads/logo.png" alt="Logo" /></a>
                </div>
            </div>
        </div>
        <!-- / content top -->
        <!--content middle -->
        <div class="gt-container">
            <div class="gtbox-12 gtbox-tablet-12">
                <div class="inner">
                    <h1 class="title">GString</h1>
                    <ul>
                        <li><a href="/templates/T01.htm">T01 - Grid</a></li>
                        <li><a href="/templates/T02.htm">T02 - Carousal</a></li>
                        <li><a href="/templates/T03.htm">T03 - Grid</a></li>
                        <li><a href="/templates/T04.htm">T04 - LightBox Trigger</a></li>
                        <li><a href="/templates/T05.htm">T05 - LightBox Overlay</a></li>
                    </ul>
                </div>
                <div class="clearfix">
                </div>
            </div>
            <!-- / content left-->
            <div class="clearfix">
            </div>
        </div>
        <!--/ content middle -->
        <!--content middle -->
        <div class="gt-container">
            <div class="gtbox-12 gtbox-tablet-12">
                <div class="inner">
                    <h2 class="title">Carousel</h2>

                    <div class='sliderContainer'>
                        <div class='iosSlider'>
                            <div class='slider'>
                                <div class='item item1'>
                                    <img src="http://placehold.it/464x115/c76/fff&text=IMG1" width="100%" /></div>
                                <div class='item item2'>
                                    <img src="http://placehold.it/464x115/B66/fff&text=IMG2" width="100%" />
                                </div>
                                <div class='item item3'>
                                    <img src="http://placehold.it/464x115/F66/fff&text=IMG3" width="100%" />
                                </div>
                                <div class='item item4'>
                                    <img src="http://placehold.it/464x115/J66/fff&text=IMG4" width="100%" />
                                </div>
                                <div class='item item5'>
                                    <img src="http://placehold.it/464x115/k96/fff&text=IMG5" width="100%" />
                                </div>
                            </div>
                            <div class='prevContainer'>
                                <div class='prev'>
                                    <i class="fa fa-arrow-left"></i>Prev slide
                                </div>
                            </div>
                            <div class='nextContainer'>
                                <div class='next'>
                                    Next slide <i class="fa fa-camera-retro"></i>
                                </div>
                            </div>
                            <div class='selectorsBlock'>
                                <div class='selectors'>
                                    <div class='item first selected'>
                                    </div>
                                    <div class='item'>
                                    </div>
                                    <div class='item'>
                                    </div>
                                    <div class='item'>
                                    </div>
                                    <div class='item'>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix">
                </div>
            </div>
            <!-- / content left-->
            <div class="clearfix">
            </div>
        </div>
        <!--/ content middle -->
    </div>
    <!--/ content -->
</body>
</html>
